// Second Level
		if(!level_one_activate && hero.center.y > 11.6f){
			world_bottom_edge = 7.1f;
			world_bottom_bound = world_bottom_edge + 3.0f;

			cloud_platforms.add(treetop_valley_factory.createStaticCloudPlatform(36.5f, 9.1f));
			level_one_activate = true;
		}
		
		// Third Level
		if(!level_two_activate && hero.center.y > 19f){
			world_bottom_edge = 14.5f;
			world_bottom_bound = world_bottom_edge + 3.0f;
			
			cloud_platforms.add(treetop_valley_factory.createStaticCloudPlatform(0.2f, 16.6f));
			level_two_activate = true;
		}
